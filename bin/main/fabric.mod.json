{
  "schemaVersion": 1,
  "id": "findyourway",
  "version": "${version}",

  "name": "Find Your Way",
  "description": "This mod adds several new compasses to help you find your way around Minecraft!",
  "authors": [
    "Yavin7!"
  ],
  "contact": {
    "homepage": "https://fabricmc.net/",
    "sources": "https://github.com/FabricMC/fabric-example-mod"
  },

  "license": "CC0-1.0",
  "icon": "assets/findyourway/icon.png",

  "environment": "*",
  "entrypoints": {
    "main": [
      "net.yseven.findyourway.FindYourWay"
    ],
    "client": [
      "net.yseven.findyourway.client.FindYourWayClientInit"
    ]
  },
  "mixins": [
    "findyourway.mixins.json"
  ],

  "depends": {
